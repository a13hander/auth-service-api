{
  "swagger": "2.0",
  "info": {
    "title": "Auth service",
    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "AuthV1"
    }
  ],
  "host": "localhost:8080",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/auth/v1/user-create": {
      "post": {
        "summary": "Создает пользователя",
        "operationId": "AuthV1_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/auth_v1CreateResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/auth_v1CreateRequest"
            }
          }
        ],
        "tags": [
          "AuthV1"
        ]
      }
    },
    "/auth/v1/user-list": {
      "post": {
        "summary": "Список пользователей",
        "operationId": "AuthV1_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/auth_v1ListResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {}
            }
          }
        ],
        "tags": [
          "AuthV1"
        ]
      }
    }
  },
  "definitions": {
    "auth_v1CreateRequest": {
      "type": "object",
      "properties": {
        "user": {
          "$ref": "#/definitions/auth_v1UserInfo"
        },
        "password": {
          "type": "string"
        },
        "passwordConfirm": {
          "type": "string"
        }
      }
    },
    "auth_v1CreateResponse": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "auth_v1Engineer": {
      "type": "object",
      "properties": {
        "level": {
          "type": "string",
          "format": "int64"
        },
        "company": {
          "type": "string"
        },
        "language": {
          "type": "string"
        }
      }
    },
    "auth_v1ListResponse": {
      "type": "object",
      "properties": {
        "user": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/auth_v1User"
          }
        }
      }
    },
    "auth_v1Manager": {
      "type": "object",
      "properties": {
        "level": {
          "type": "string",
          "format": "int64"
        },
        "company": {
          "type": "string"
        },
        "experience": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "auth_v1Role": {
      "type": "string",
      "enum": [
        "USER",
        "ADMIN"
      ],
      "default": "USER"
    },
    "auth_v1User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "info": {
          "$ref": "#/definitions/auth_v1UserInfo"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "auth_v1UserInfo": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "role": {
          "$ref": "#/definitions/auth_v1Role"
        },
        "engineer": {
          "$ref": "#/definitions/auth_v1Engineer"
        },
        "manager": {
          "$ref": "#/definitions/auth_v1Manager"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
